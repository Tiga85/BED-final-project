{
  "name": "express-bookings",
  "version": "1.0.0",
  "type": "module",
  "description": "Your REST API for managing bookings",
  "main": "index.js",
  "prisma": {
    "seed": "node ./prisma/seed.js"
  },
  "scripts": {
    "dev": "nodemon index.js",
    "test-positive": "newman run \"./postman/collections/Bookings API.json\" -e \"./postman/environments/Local.postman_environment.json\"",
    "test-negative": "newman run \"./postman/collections/Bookings API Negative.json\" -e \"./postman/environments/Local.postman_environment.json\"",
    "test": "npm run test-positive && npm run test-negative"
  },
  "author": "Tiga85",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.19.1",
    "@sentry/nextjs": "^8.29.0",
    "@sentry/node": "^7.119.0",
    "@sentry/profiling-node": "^8.28.0",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.4.5",
    "express": "^5.0.0",
    "express-oauth2-jwt-bearer": "^1.6.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "next": "^14.2.8",
    "request": "^2.88.2",
    "uuid": "^9.0.1",
    "winston": "^3.14.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/request": "^2.48.12",
    "@types/uuid": "^10.0.0",
    "newman": "^6.0.0",
    "nodemon": "^3.0.1",
    "prisma": "^5.19.1"
  },
  "engines": {
    "node": ">=18 <19"
  },
  "keywords": []
}
